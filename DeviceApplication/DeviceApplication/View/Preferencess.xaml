<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DeviceApplication.View.Preferencess">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="txtValue" Placeholder="Add Value"></Entry>
            <Button x:Name="AddButton" Text="Add" Clicked="Add_button_clicked"/>
            <Button x:Name="GetButton" Text="Get" Clicked="Get_Button_Clicked" />
            <Button x:Name="RemoveButton" Text="Remove" Clicked="Remove_Button_Clicked" />
            <Button x:Name="ClearButton" Text="Clear All"  Clicked="Clear_Button_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DeviceApplication.View.AppInformation">
    <ContentPage.Content>
        <StackLayout>
            <StackLayout Margin="10,10,10,10">
                <Label FontAttributes="Bold" Text="Getting Device information" VerticalOptions="Center" HorizontalOptions="Start" />
                <Label HorizontalOptions="Start" x:Name="lblAppName"></Label>
                <Label HorizontalOptions="Start" x:Name="lblPackageName"></Label>
                <Label HorizontalOptions="Start" x:Name="lblVersion"></Label>
                <Label HorizontalOptions="Start" x:Name="lblBuildNumber"></Label>
                
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>